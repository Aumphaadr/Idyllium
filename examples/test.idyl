// draw.idyl
use console;

void function line(int L)
{
    string res = "";
    for (int i = 0; i < L; i = i + 1)
    {
        res = res + '-';
    }
    console.write(res, '\n');
}


// main.idyl
use console;
use draw; // наша собственная библиотека

main() {
    // вызов функции 'line()' из библиотеки 'draw'
    draw.line(10);
}